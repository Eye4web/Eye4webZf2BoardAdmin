{
    "name": "eye4web/eye4web-zf2-board-admin",
    "description": "Administration for Eye4web\\Zf2Board",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf2",
        "board",
        "forum",
        "admin"
    ],
    "homepage": "https://github.com/Eye4web/Eye4webZf2BoardAdmin",
    "authors": [
        {
            "name": "Daniel Strøm",
            "email": "ds@eye4web.dk"
        },
        {
            "name": "Nikolaj Løvenhardt Petersen",
            "email": "np@eye4web.dk"
        }
    ],
    "require": {
        "php": "^8.1",
        "laminas/laminas-mvc": "^3.0",
        "eye4web/eye4web-zf2-board": "dev-laminas3",
        "zf-commons/zfc-admin": "dev-laminas3",
        "cocur/slugify": "0.10.*"
    },
    "autoload": {
        "psr-0": {
            "Eye4web\\Zf2Board": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "require-dev": {
        "phpmd/phpmd" : "1.4.*",
        "phpunit/phpunit" : "4.*",
        "squizlabs/php_codesniffer" : "1.5.*",
        "zendframework/zend-captcha" : "~2.1",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/samsonasik/ZfcAdmin",
            "no-api": true
        }
    ]
}
